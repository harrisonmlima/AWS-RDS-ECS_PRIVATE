[  
  {
    "name": "${APP_NAME}",
    "image": "233181867717.dkr.ecr.us-east-1.amazonaws.com/web-ecr:latest",
    "essential":true,
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "${REGION}",
        "awslogs-stream-prefix": "app-logstream",
        "awslogs-group": "${LOG_GROUP}"
      }
    }
  }
]